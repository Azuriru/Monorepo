{
	"compilerOptions": {
		"jsx": "react",
		"allowJs": false,
		"allowImportingTsExtensions": true,
		"checkJs": false,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"noImplicitAny": false,
		"noEmit": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"strictNullChecks": true,
		"verbatimModuleSyntax": false,
		"lib": [
			"es2015",
			"dom"
		]
	},
}